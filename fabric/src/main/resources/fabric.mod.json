{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_website}",
    "sources": "${mod_repository}"
  },
  "license": "${mod_license}",
  "icon": "assets/guildmasters/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.daqem.guildmasters.fabric.client.GuildMastersFabricClient"
    ],
    "main": [
      "com.daqem.guildmasters.fabric.GuildMastersFabric"
    ]
  },
  "mixins": [
    "guildmasters.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_api_version}",
    "uilib": ">=${ui_library_version}",
    "yamlconfig": ">=${config_library_version}",
    "arc": ">=${arc_version}"
  }
}